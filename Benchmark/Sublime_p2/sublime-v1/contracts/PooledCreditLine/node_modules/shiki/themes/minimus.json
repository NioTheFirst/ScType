{
  "name": "minimus",
  "type": "light",
  "tokenColors": [
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#969896"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.regexp", "constant.other.symbol"],
      "settings": {
        "foreground": "#868e96"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#343a40"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": ["constant.numeric", "constant.character", "constant.keyword", "constant"],
      "settings": {
        "foreground": "#868e96"
      }
    },
    {
      "name": "Global definitions",
      "scope": "entity.name",
      "settings": {
        "foreground": "#343a40",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Global constant definitions",
      "scope": [
        "variable.other.constant.js",
        "variable.other.constant.jsx",
        "variable.other.constant.ts",
        "variable.other.constant.tsx"
      ],
      "settings": {
        "foreground": "#343a40"
      }
    },
    {
      "name": "Global definitions",
      "scope": ["entity.name.type.ts", "entity.name.type.tsx"],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#343a40"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
      "settings": {
        "foreground": "#343a40"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "foreground": "#343a40"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "foreground": "#343a40"
      }
    },
    {
      "name": "Built-in Constants",
      "scope": "constant.language",
      "settings": {
        "foreground": "#343a40",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#343a40",
        "fontStyle": " italic"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#343a40",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#343a40",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#969896",
        "fontStyle": "italic"
      }
    },
    {
      "name": "as, return keywords",
      "scope": [
        "keyword.control.as.js",
        "keyword.control.as.jsx",
        "keyword.control.as.ts",
        "keyword.control.as.tsx",
        "keyword.control.flow.js",
        "keyword.control.flow.jsx",
        "keyword.control.flow.ts",
        "keyword.control.flow.tsx"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid, Deprecated",
      "scope": ["invalid", "deprecated", "support.deprecated", "invalid.deprecated"],
      "settings": {
        "foreground": "#9775fa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSON Property Names' Quotation Marks",
      "scope": [
        "support.type.property-name.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#3a3a3a90"
      }
    },
    {
      "name": "JSON Values",
      "scope": [
        "string.quoted.double.json",
        "punctuation.definition.string.begin.json",
        "punctuation.definition.string.end.json"
      ],
      "settings": {
        "foreground": "#3a3a3a"
      }
    }
  ],
  "colors": {
    "foreground": "#343a40",
    "focusBorder": "#343a40",
